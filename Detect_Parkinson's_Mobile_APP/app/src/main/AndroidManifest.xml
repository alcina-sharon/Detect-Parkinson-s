<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.detectparkinson.john">
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/png"
        android:label="Detect Parkinson's"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.detectparkinson.john.activities.LoginActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name="com.detectparkinson.john.activities.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.detectparkinson.john.activities.UsersListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.detectparkinson.john.Main2Activity"
            android:label="Detect Parkinson's"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.detectparkinson.john.test1" />
        <activity android:name="com.detectparkinson.john.test2" />
        <activity android:name="com.detectparkinson.john.fb" />
        <activity android:name="com.detectparkinson.john.splash"><intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
    </application>

</manifest>